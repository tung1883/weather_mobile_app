{
  "expo": {
    "name": "sample1",
    "slug": "sample1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [],
      "package": "com.sample1",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization",
      [
        "react-native-android-widget",
        {
          "widgets": [
            {
              "name": "Small",
              "label": "Small Widget",
              "minWidth": "180dp",
              "minHeight": "30dp",
              "previewImage": "./assets/widget-preview/small.png",
              "updatePeriodMillis": 1800000
            },
            {
              "name": "Big",
              "label": "Big Widget",
              "minWidth": "240dp",
              "minHeight": "80dp",
              "previewImage": "./assets/widget-preview/big.png",
              "updatePeriodMillis": 1800000
            }
          ]
        }
      ]
    ]
  }
}

